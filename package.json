{
  "name": "singbox-nodejs-warp-xray-tunnel",
  "version": "1.0.0",
  "description": "TUIC + Hysteria2 + Reality·北京时间零点重启",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "start": "node index.js",
    "config:webdav-port": "node -e \"import('./config.js').then(m => console.log(m.getconfig().WEBDAV_PORT))\"",
    "config:webdav-path": "node -e \"import('./config.js').then(m => console.log(m.getconfig().WEBDAV_PATH))\"",
    "config:webdav-username": "node -e \"import('./config.js').then(m => console.log(m.getconfig().WEBDAV_USERNAME))\"",
    "config:webdav-password": "node -e \"import('./config.js').then(m => console.log(m.getconfig().WEBDAV_PASSWORD))\""
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@masx200/webdav-cli": "^3.1.2",
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "unzipper": "^0.12.3",
    "uuid": "^13.0.0"
  }
}
